{
  "name": "lackey-backpack",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run config:dev && vite --mode development",
    "dev:staging": "npm run config:staging && vite --mode staging",
    "dev:prod": "npm run config:prod && vite --mode production",
    "build": "npm run config:prod && vite build --mode production",
    "build:dev": "npm run config:dev && vite build --mode development",
    "build:staging": "npm run config:staging && vite build --mode staging",
    "build:prod": "npm run config:prod && vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:dev": "vite preview --mode development",
    "preview:staging": "vite preview --mode staging",
    "preview:prod": "vite preview --mode production",
    "start:partitions": "cd backend && npm run start:partitions",
    "config:dev": "node scripts/setup-config.js development",
    "config:staging": "node scripts/setup-config.js staging",
    "config:prod": "node scripts/setup-config.js production",
    "test": "vitest",
    "test:unit": "vitest run unit",
    "test:integration": "vitest run integration",
    "test:e2e": "playwright test",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "backend:dev": "cd backend && npm run dev",
    "backend:staging": "cd backend && npm run start:staging",
    "backend:prod": "cd backend && npm run start:prod",
    "setup:dev": "npm run config:dev && cd backend && npm run setup:dev",
    "setup:staging": "npm run config:staging && cd backend && npm run setup:staging",
    "setup:prod": "npm run config:prod && cd backend && npm run setup:prod"
  },
