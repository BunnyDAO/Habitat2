    "@prisma/client": "^6.6.0",
    "@pythnetwork/hermes-client": "^2.0.0",
    "@solana/spl-token": "^0.3.9",
    "@solana/web3.js": "^1.87.6",
    "@supabase/supabase-js": "^2.49.4",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "axios": "^1.8.4",
    "bs58": "^4.0.1",
    "tweetnacl": "^1.0.3",
    "cors": "^2.8.5",
    "cron": "^3.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.14.1",
    "redis": "^4.6.11",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/bs58": "^4.0.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.17.30",
    "@types/pg": "^8.11.11",
    "@types/supertest": "^6.0.2",
