  "description": "Backend service for Habitat",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:daemon": "jest --testPathPattern=daemon",
    "test:workers": "jest --testPathPattern=workers",
    "start:partitions": "ts-node src/scripts/manage_partitions.ts",
    "update-tokens": "ts-node src/scripts/update-token-data.ts",
    "update-prices": "ts-node src/scripts/update-token-prices.ts",
    "update-xstock-tokens": "ts-node src/scripts/update-xstock-tokens.ts",
    "run-schema": "ts-node src/scripts/run-schema.ts",
    "initialize-tokens": "ts-node src/scripts/initialize-tokens.ts",
    "start:strategy-executor": "ts-node src/scripts/run-strategy-executor.ts",
    "start:strategy-migration": "ts-node src/scripts/run-strategy-migration.ts",
    "check-strategies": "ts-node src/scripts/check-strategies.ts",
    "start:daemon": "ts-node src/scripts/strategy-daemon.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
